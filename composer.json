{
    "name": "oidc/oauth2",
    "description": "The library for OAuth 2.0 Authorization Framework.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oauth2"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/oidcphp/oauth2/issues"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "guzzlehttp/guzzle": "^6.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "illuminate/container": "^5.6 | ^6 | ^7",
        "illuminate/support": "^5.6 | ^6 | ^7",
        "laminas/laminas-diactoros": "^2.2",
        "mileschou/mocker": "^0.1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7 | ^8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\OAuth2\\": "src/OAuth2"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
