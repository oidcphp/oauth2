{
    "name": "oidc/oauth2",
    "description": "The library for OAuth 2.0 Authorization Framework.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oauth2"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/oidcphp/oauth2/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.5",
        "illuminate/container": "^9 | ^10",
        "illuminate/support": "^9 | ^10",
        "laminas/laminas-diactoros": "^2.2",
        "mileschou/mocker": "^1.1",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\OAuth2\\": "src/OAuth2"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
